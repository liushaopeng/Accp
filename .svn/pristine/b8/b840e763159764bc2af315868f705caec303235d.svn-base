	}
	public void setLsRecords(List<AccRecord> lsRecords) {
		this.lsRecords = lsRecords;
	}
	public double getOther() {
		return other;
	}
	public void setOther(double other) {
		this.other = other;
	}
	public int getState() {
		return state;
