	/**
	 * 作废说明
	 */
	private String invalids;
	public String getCustid() {
		return custid;
	}
	public void setCustid(String custid) {
		this.custid = custid;
	}
	public String getGid() {
		return gid;
	}
	public void setGid(String gid) {
		this.gid = gid;
	}
	public Long getDid() {
		return did;
	}
	public void setDid(Long did) {
		this.did = did;
	}
	public String getRamadhin() {
		return ramadhin;
	}
	public void setRamadhin(String ramadhin) {
		this.ramadhin = ramadhin;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
