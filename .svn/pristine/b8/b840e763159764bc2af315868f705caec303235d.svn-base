	}
	public double getTotal() {
		return total;
	}
	public void setTotal(double total) {
		this.total = total;
	}
	public double getPaidin() {
		return paidin;
	}
	public void setPaidin(double paidin) {
		this.paidin = paidin;
	}
	public String getPicurl() {
		return picurl;
	}
