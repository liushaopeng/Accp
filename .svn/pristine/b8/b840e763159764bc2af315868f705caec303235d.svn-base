package com.lsp.acc.entity;

import java.util.Date;
import java.util.List;

import com.mongodb.ReflectionDBObject;
/**
 * 账单
 * @author lsp
 *
 */
public class Accounting extends ReflectionDBObject{

	private String custid;
	/**
	 * 商品
	 */
